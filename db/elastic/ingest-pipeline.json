{
  "description": "Standardize timestamp and set processed_at",
  "processors": [
    {
      "date": {
        "field": "timestamp",
        "target_field": "timestamp",
        "formats": ["ISO8601"],
        "ignore_failure": false
      }
    },
    {
      "set": {
        "field": "processed_at",
        "value": "{{_ingest.timestamp}}"
      }
    }
  ]
}
